    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "cypress:open": "cd .. && NEXT_PUBLIC_NODE_TEST=true NODE_TEST=true yarn run build && concurrently -k -s first 'NODE_TEST=true yarn start' 'cd lumium-space && NODE_TEST=true cypress open'",
