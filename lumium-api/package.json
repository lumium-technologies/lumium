{
    "name": "lumium-api",
    "version": "0.0.1",
    "description": "",
    "scripts": {
        "test": "jest --passWithNoTests --coverage",
        "build": "tsc",
        "start": "npm run typeorm migration:run && ts-node src/index.ts",
        "start:dev": "concurrently -n express,jest \"nodemon src/index.ts\" \"jest --watchAll --verbose --passWithNoTests --coverage\"",
        "dev": "source ./scripts/localEnv.sh && npm run typeorm migration:run && npm run start:dev",
        "typeorm": "node --require ts-node/register ../node_modules/typeorm/cli.js -d ./src/data-source.ts"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/cors": "^2.8.12",
        "concurrently": "^7.0.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.0",
        "express": "^4.17.3",
        "express-jsdoc-swagger": "^1.6.7",
        "express-rate-limit": "^6.3.0",
        "jest": "^27.5.1",
        "node-pg-migrate": "^6.2.1",
        "pg": "^8.4.0",
        "reflect-metadata": "^0.1.13",
        "supertokens-node": "^9.0.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/node": "^16.11.10",
        "@typescript-eslint/eslint-plugin": "^5.17.0",
        "@typescript-eslint/parser": "^5.17.0",
        "eslint": "^8.12.0",
        "nodemon": "^2.0.15",
        "ts-node": "^10.7.0",
        "typeorm": "^0.3.6",
        "typescript": "4.5.2"
    }
