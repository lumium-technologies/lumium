        "cypress:test": "cd .. && NEXT_PUBLIC_NODE_TEST=true NODE_TEST=true yarn run build && concurrently -k -s first 'NODE_TEST=true yarn start' 'cd lumium-space && yarn run cypress:run'",
        "lumium-renderer:package": ". ./scripts/build-renderer.sh",
        "preinstall": "yarn run lumium-renderer:package"
    },
